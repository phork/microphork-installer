{
    "name": "microphork/package-installer",
    "description": "The package installer for the microphork framework",
    "keywords": ["microphork", "phork", "framework"],
    "license": "MIT",
    "homepage": "http://phork.org",
    "authors": [
        {
            "name": "Elenor Collings",
            "email": "hello@elenor.net",
            "homepage": "http://elenor.net",
            "role": "Developer"
        }
    ],
    "type": "composer-plugin",
    "autoload": {
        "psr-4": {
            "Phork\\Composer\\": "php/classes"
        }
    },
    "extra": {
        "class": "Phork\\Composer\\InstallerPlugin",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "composer-plugin-api": "1.0.0"
    }
}