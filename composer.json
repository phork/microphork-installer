{
    "name": "microphork/package-installer",
    "description": "The package installer for the microphork framework.",
    "type": "composer-plugin",
    "license": "MIT",
    "autoload": {
        "psr-0": {"Phork\\Composer\\": "src/"}
    },
    "extra": {
        "class": "Phork\\Composer\\InstallerPlugin",
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "require": {
        "composer-plugin-api": "1.0.0"
    }
}